{
  "workflows": {
    "draft": ["draft", "needs_review"],
    "needs_review": ["draft", "needs_review", "published"],
    "published": ["draft", "published", "archived"],
    "archived": ["draft", "archived"]
  },
  "roles": {
    "member": {
      "default_workflow_status": "draft",
      "workflow_status_options": []
    },
    "editor": {
      "default_workflow_status": "draft",
      "workflow_status_options": ["draft", "needs_review", "archived"]
    },
    "admin": {
      "default_workflow_status": "draft",
      "workflow_status_options": ["draft", "needs_review", "published", "archived"]
    }
  }
}